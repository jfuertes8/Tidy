{
  "expo": {
    "name": "Tidy",
    "slug": "Tidy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "App project using React Native. The basic idea is an app which let's you manage your wardrobes and clothes (ideal for when you are living in a different city from the one where you were born, and maybe travel back and forth).",
    "githubUrl": "https://github.com/jfuertes8"
  }
}
